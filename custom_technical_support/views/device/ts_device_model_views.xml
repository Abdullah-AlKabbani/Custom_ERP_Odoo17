<odoo>
    <record id="view_ts_device_model_tree" model="ir.ui.view">
        <field name="name">ts.device.model.tree</field>
        <field name="model">ts.device.model</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
                <field name="brand_id"/>
                <field name="category_id"/>
                <field name="active"/>
            </tree>
        </field>
    </record>
</odoo>
