<odoo>
    <record id="view_ts_device_brand_tree" model="ir.ui.view">
        <field name="name">ts.device.brand.tree</field>
        <field name="model">ts.device.brand</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
